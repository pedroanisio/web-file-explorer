{
    "id": "pydantic_ai_agent",
    "name": "PydanticAI Agent",
    "version": "1.0.0",
    "description": "Integration with PydanticAI for intelligent code analysis and assistance",
    "entry_point": "agent_plugin",
    "icon": "ðŸ¤–",
    "type": "backend",
    "hooks": [
        "file_processor",
        "query_handler",
        "startup"
    ],
    "dependencies": [
        "pydantic>=2.0.0",
        "pydantic-ai>=0.1.3"
    ],
    "settings": {
        "llm_provider": "openai",
        "api_key_env": "OPENAI_API_KEY",
        "model": "gpt-4",
        "max_tokens": 4096,
        "temperature": 0.7
    },
    "auto_install_dependencies": false
}
